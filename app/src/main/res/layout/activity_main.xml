<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/hmbackground"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

    <Button
        android:id="@+id/recipespage"
        android:layout_width="172dp"
        android:layout_height="69dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="341dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="35dp"
        android:text="Recipes"
        app:layout_constraintBottom_toTopOf="@+id/goalspage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goalspage"
        android:layout_width="171dp"
        android:layout_height="71dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="45dp"
        android:text="Goals"
        app:layout_constraintBottom_toTopOf="@+id/discoverpage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recipespage" />

    <Button
        android:id="@+id/discoverpage"
        android:layout_width="169dp"
        android:layout_height="71dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="99dp"
        android:text="Discover"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goalspage"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="425dp"
            android:layout_height="67dp"
            android:layout_marginStart="6dp"
            android:layout_marginTop="96dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="153dp"
            android:background="#D237C83D"
            android:clickable="true"
            android:gravity="center"
            android:text="GOURMET HEALTH"
            android:textColor="#0B0000"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/recipespage"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>